 <Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

    <Script File="Functions_ui.lua"/>
    <Frame name="TeebsClassic_MainFrame" parent="UIParent">
        <Size x="500" y="500"/>
        <Anchors>
            <Anchor point="CENTER" relativePoint="CENTER" relativeTo="UIParent"/>
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture name="$parent_background" parentKey="background" file="Interface\DialogFrame\UI-DialogBox-Background">
                    <Size x="500" y="500"/>
                    <Anchors>
                         <Anchor point="TOPLEFT"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <Texture name="$parent_Portrait" parentKey="portrait" file="Interface\Icons\trade_engineering">
                    <Size x="60" y="60"/>
                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset x="7" y="-6"/>
                        </Anchor>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
                <FontString name="$parent_Title" parentKey="title" inherits="GameFontNormal" text="TeebsClassic">
                    <Anchors>
                        <Anchor point="TOP">
                            <Offset x="0" y="-18"/>
                        </Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad function="TeebsClassic_OnLoad"/>
        </Scripts>
    </Frame>
</Ui>